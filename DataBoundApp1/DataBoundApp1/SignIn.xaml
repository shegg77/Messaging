					<StackPanel VerticalAlignment="Center">
						<Border BorderThickness="0,0,0,2" BorderBrush="#4C000000" Padding="5">
							<Controls:WatermarkTextBox x:Name="txtUserName" BorderBrush="{x:Null}" BorderThickness="3" WatermarkText="username" Style="{StaticResource WatermarkTextBoxStyle2}" InputScope="Default" FontFamily="/DataBoundApp1;component/Resources/Fonts.zip#Lato Light"/>
						</Border>
						<Border BorderThickness="0" BorderBrush="#4C000000" Padding="5">
							<Grid>
								<TextBlock x:Name="passwordHint" HorizontalAlignment="Left" TextWrapping="Wrap" Text="password" VerticalAlignment="Center" Margin="70,0,0,5" FontFamily="/DataBoundApp1;component/Resources/Fonts.zip#Lato Light" Foreground="#FF868686" Visibility="Visible" FontSize="24" />
								<PasswordBox x:Name="txtPassword" Style="{StaticResource PasswordBoxStyle1}" Background="{x:Null}" LostFocus="txtPassword_LostFocus" GotFocus="txtPassword_GotFocus"/>
							</Grid>
						</Border>
					</StackPanel>
				</StackPanel>
			</Border>
			<StackPanel Loaded="stackPanel_Loaded">
				<Button Height="100" Background="#FF1ABC9C" BorderBrush="#FF1ABC9C" FontFamily="/DataBoundApp1;component/Resources/Fonts.zip#Lato Light" Tap="Button_Tap">
					<TextBlock TextWrapping="Wrap" Text="Log In" FontSize="36"/>
				</Button>
                <TextBlock
                    x:Name="txtbRegister"
			    TextWrapping="Wrap"            
			    VerticalAlignment="Top"
			    Foreground="#00000000" FontFamily="/DataBoundApp1;component/Resources/Fonts.zip#Lato" FontSize="20" Margin="0,24,0,0" HorizontalAlignment="Center" Visibility="Visible" Tap="txtbRegister_Tap" Opacity="0">
            
                <Run Text="Don't have an account? "/>
                <Run Text="Register." FontFamily="/DataBoundApp1;component/Resources/Fonts.zip#Lato Black" FontSize="36" Foreground="#FFDC5452"/>
                </TextBlock>
            </StackPanel>
		</StackPanel>
        
        
    </Grid>

</phone:PhoneApplicationPage>